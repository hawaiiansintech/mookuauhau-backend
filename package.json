{
  "name": "mookuauhau-backend",
  "version": "0.1.2",
  "description": "read GEDCOM file and load into backend (neo4j or graphql), also createion of hasura graphql backend",
  "source": "index.ts",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "show": "node -r dotenv/config ./dist/index.js",
    "load": "ts-node-esm -r dotenv/config ./src/astload.js",
    "compactload": "ts-node-esm -r dotenv/config ./src/compactload.js",
    "queueload": "ts-node-esm -r dotenv/config ./src/queueload.js",
    "t4j": "ts-node-esm -r dotenv/config src/neo4jtest.ts",
    "tgql": "ts-node-esm -r dotenv/config src/graphqltest.ts",
    "cli": "ts-node src/cli.ts",
    "test": "npm run build && uvu tests",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node",
    "ts-node-esm": "ts-node-esm",
    "hasura-cli": "docker compose run hasura-cli"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nhost/nhost-js": "^1.4.5",
    "dotenv": "^16.0.1",
    "graphql-request": "^4.3.0",
    "neo4j-driver": "^4.4.6",
    "node-fetch": "^3.2.8",
    "parse-gedcom": "^2.0.1"
  },
  "devDependencies": {
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "uvu": "^0.5.6"
  }
}
